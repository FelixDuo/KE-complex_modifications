{
	"title": "Hai HHKB Launch app (iPhone Mirroring)",
	"rules": [
    	{
			"description": "right_option + I -> iPhone Mirroring",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "i",
						"modifiers": {
							"mandatory": ["right_option"]
						}
					},
					"to": [
						{
							"shell_command": "open '/System/Applications/iPhone Mirroring.app'"
						}
					]
				}
			]
		}
  ]
}
