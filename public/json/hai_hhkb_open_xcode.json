{
	"title": "Hai HHKB Launch app (Xcode)",
	"rules": [
		{
			"description": "right_option + X -> Xcode",
			"manipulators": [
				{
					"type": "basic",
					"from": {
						"key_code": "x",
						"modifiers": {
							"mandatory": ["right_option"]
						}
					},
					"to": [
						{
							"shell_command": "open -a Xcode"
						}
					]
				}
			]
		}
	]
}
